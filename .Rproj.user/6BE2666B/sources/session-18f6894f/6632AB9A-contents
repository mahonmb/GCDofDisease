                  mods = ~ Macroparasite * Global.Change.Driver,
                  random = list(~1|Citation.number, ~1|ind_id),
                  data=datMacroparasite, method = "ML",
                    control=list(optimizer="optimParallel", ncpus=15))

#Difference
anova(mMacro, mMacroIntML)

